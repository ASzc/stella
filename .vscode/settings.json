// Platzieren Sie Ihre Einstellungen in dieser Datei, um Standard- und Benutzereinstellungen zu Ã¼berschreiben.
{
    "editor.tabSize": 2,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "src/**/*.o": true
    },
    "editor.trimAutoWhitespace": true,
    "editor.useTabStops": false,
    "C_Cpp.intelliSenseEngine": "Default",
    "files.insertFinalNewline": true
}
